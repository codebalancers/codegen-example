{
  "name": "codegen-example",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/codebalancers/codegen-example",
  "author": "codebalancers",
  "license": "MIT",
  "description": "Codegen example",
  "scripts": {
    "clean": "rm -rf build/",
    "prep-dir": "mkdir -p build/",
    "compile": "tsc",
    "lint": "tslint --project tslint.json codegen/**/*.ts",
    "build": "yarn run clean && yarn run prep-dir && yarn run lint && yarn run compile",
    "generate": "codegen generate -m ./model -c codegen.json -f ./build/filters/index.js --mp ./build/model-processors/index.js",
    "schema:generate": "codegen schema:generate -s ./src/schema/v1/model.ts",
    "serve": "codegen serve -m ./model"
  },
  "dependencies": {
    "@codebalancers/codegen": "^1.0.2",
    "prettier": "^1.13.5"
  },
  "devDependencies": {
    "@types/node": "^10.3.3",
    "@types/nunjucks": "^3.0.0",
    "@types/prettier": "^1.13.1",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.1.0",
    "typescript": "2.6.2"
  }
}
